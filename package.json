    "expect": "^1.20.2",
    "mocha": "^3.2.0",
    "next": "^2.1.1",
    "nightmare": "^2.10.0",
    "nodemon": "^1.11.0",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "rimraf": "^2.6.1",
